--
-- Table structure for table `wp_slim_outbound`
--

DROP TABLE IF EXISTS `wp_slim_outbound`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `wp_slim_outbound` (
  `outbound_id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `outbound_domain` varchar(255) DEFAULT '',
  `outbound_resource` varchar(2048) DEFAULT '',
  `type` tinyint(3) unsigned DEFAULT '0',
  `notes` varchar(512) DEFAULT '',
  `position` varchar(32) DEFAULT '',
  `id` int(10) unsigned NOT NULL DEFAULT '0',
  `dt` int(10) unsigned DEFAULT '0',
  PRIMARY KEY (`outbound_id`),
  KEY `idx_wp_slim_outbound` (`dt`),
  KEY `fk_wp_id` (`id`),
  CONSTRAINT `fk_wp_id` FOREIGN KEY (`id`) REFERENCES `wp_slim_stats` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
/*!40101 SET character_set_client = @saved_cs_client */;

--
-- Dumping data for table `wp_slim_outbound`
--

LOCK TABLES `wp_slim_outbound` WRITE;
/*!40000 ALTER TABLE `wp_slim_outbound` DISABLE KEYS */;
/*!40000 ALTER TABLE `wp_slim_outbound` ENABLE KEYS */;
UNLOCK TABLES;

--
-- Table structure for table `wp_slim_screenres`
--

DROP TABLE IF EXISTS `wp_slim_screenres`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `wp_slim_screenres` (
  `screenres_id` mediumint(8) unsigned NOT NULL AUTO_INCREMENT,
  `resolution` varchar(12) DEFAULT '',
  `colordepth` varchar(5) DEFAULT '',
  `antialias` tinyint(1) DEFAULT '0',
  PRIMARY KEY (`screenres_id`),
  UNIQUE KEY `uk_wp_screenres` (`resolution`,`colordepth`,`antialias`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8;
